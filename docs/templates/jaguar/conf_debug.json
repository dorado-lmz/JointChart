{
	"tags": {
		"allowUnknownTags" : true
	},
  	"source": {
  	    "include": ["src"],
  	    "exclude": ["src/vectorizer.js","src/geometry.js"]
  	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : false
		},
		"applicationName": "Joint_Chart",
		"disqus": "",
		"googleAnalytics": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "",
			"description": "",
			"keyword": ""
		},
		"linenums": false
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true
	}
}